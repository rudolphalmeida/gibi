    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Run executable 'gibi' with Tetris.gb",
            "program": "target/debug/gibi",
            "args": [
                "roms/Tetris.gb",
                "-s5"
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_LOG": "debug"
            },
            "sourceLanguages": [
                "rust"
            ]
        },
